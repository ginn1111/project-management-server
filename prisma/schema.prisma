model EmployeesOfDepartment {
  id                        String                      @id(clustered: false, map: "PK_NHAN_VIEN_PHONG_BAN") @map("ID_NVPB") @db.Char(31)
  idEmployee                String?                     @map("ID_NV") @db.Char(31)
