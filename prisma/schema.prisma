model WorksOfEmployee {
  id              String                      @id(clustered: false, map: "PK_NHAN_VIEN_DAU_VIEC") @map("ID_NVDV") @db.Char(31)
  idEmployee      String?                     @map("ID_NVDA") @db.Char(31)
